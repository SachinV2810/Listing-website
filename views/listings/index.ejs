<% layout('./layouts/boilerplate.ejs')-%>
<br/>
    <div class="row row-cols-lg-3 row-cols-md-2 row-cols-sm-1">
<% for(listing of allListings ) {%>
  
    <div class="card col listing_card ">
      <a href="/listing/<%=listing._id%>" class="listing-show">
        <img src="<%=listing.image.url%>" class="card-img-top"style="height: 20rem;" alt="listing-image">
        <div class="card-img-overlay"></div>
        <div class="card-body">
          <p class="card-text">
            <h6><b><%=listing.title  %></b></h6>
            <b>&#8377;<%=listing.price.toLocaleString("en-IN")%></b>/night</p>
        </div></a>
      </div>
      <%}%>
    </div>
    
 


